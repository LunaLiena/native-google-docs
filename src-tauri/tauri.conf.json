{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Google Docs",
  "version": "0.1.0",
  "identifier": "com.docsapp",
  "build": {
    "frontendDist": "../frontend",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "label": "main",
        "title": "Google Docs",
        "width": 1400,
        "height": 900,
        "resizable": true,
        "fullscreen": false,
        "url": "https://docs.google.com/document/"
      }
    ],
    "security": {
      "csp": "default-src * 'unsafe-inline' 'unsafe-eval' data: blob:;"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}